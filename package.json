{
    "name": "tsdl-actual", 
    "version": "0.9.1",
    "dependencies": {
        "sdl2": "bsansouci/SDL-mirror.git#esy",
        "@opam/topkg": "*",
        "@opam/ctypes-foreign": "*",
        "@opam/ocamlfind": "*",
        "@opam/ctypes": ">= 0.9.0",
        "substs": "yunxing/substs.git",
        "@opam/ocamlbuild": "*",
        "@opam/result": "*",
        "opam-installer-bin": "yunxing/opam-installer-bin.git"
    },
    "esy": {
      "build": ["ocaml pkg/pkg.ml build --pinned false && (opam-installer --prefix=$opam_prefix || true)"],
      "exportedEnvVars": {
        "tsdl_installed": {
            "scope": "global",
            "val": "true"
        },
        "FINDLIB": {
            "scope": "global",
            "val": "$tsdl_actual__target_dir/ocamlfind/lib:$FINDLIB"
        },
        "version": {
            "scope": "global",
            "val": "0.9.1"
        },
        "tsdl_enable": {
            "scope": "global",
            "val": "enable"
        },
        "PATH": {
            "scope": "global",
            "val": "$tsdl_actual__target_dir/ocamlfind/bin:$PATH"
        },
        "tsdl_version": {
            "scope": "global",
            "val": "0.9.1"
        }
      }
    },
    "devDependencies": {
      "ocaml": "esy-ocaml/ocaml#esy"
    }
}
