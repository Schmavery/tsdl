{
    "name": "tsdl-actual",
    "exportedEnvVars": {
        "tsdl_installed": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "true"
        },
        "FINDLIB": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/lib"
        },
        "version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "0.9.1"
        },
        "tsdl_enable": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "enable"
        },
        "PATH": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/bin"
        },
        "tsdl_version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "0.9.1"
        }
    },
    "version": "0.9.1",
    "dependencies": {
        "sdl2": "https://github.com/bsansouci/SDL-mirror.git#packagejson-2.0.5",
        "@opam-alpha/topkg": "*",
        "@opam-alpha/ctypes-foreign": "*",
        "@opam-alpha/ocamlfind": "*",
        "@opam-alpha/ctypes": ">= 0.9.0",
        "dependency-env": "https://github.com/npm-ml/dependency-env.git",
        "substs": "https://github.com/yunxing/substs.git",
        "@opam-alpha/ocamlbuild": "*",
        "@opam-alpha/result": "*",
        "nopam": "https://github.com/yunxing/nopam.git",
        "opam-installer-bin": "https://github.com/yunxing/opam-installer-bin.git"
    },
    "scripts": {
        "postinstall": "eval $(dependencyEnv) && nopam && ocaml pkg/pkg.ml build --pinned false && (opam-installer --prefix=$opam_prefix || true)"
    },
    "peerDependencies": {
        "@opam-alpha/ocaml": ">= 4.01.0"
    }
}
