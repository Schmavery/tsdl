{
    "name": "tsdl-actual", 
    "version": "0.9.1",
    "dependencies": {
        "sdl2": "https://github.com/bsansouci/SDL-mirror.git#esy",
        "@opam/topkg": "*",
        "@opam/ctypes-foreign": "*",
        "@opam/ocamlfind": "*",
        "@opam/ctypes": ">= 0.9.0",
        "substs": "https://github.com/yunxing/substs.git",
        "@opam/ocamlbuild": "*",
        "@opam/result": "*",
        "opam-installer-bin": "https://github.com/yunxing/opam-installer-bin.git"
    },
    "esy": {
      "build": ["ocaml pkg/pkg.ml build --pinned false && (opam-installer --prefix=$opam_prefix || true)"],
      "exportedEnvVars": {
        "tsdl_installed": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "true"
        },
        "FINDLIB": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/lib"
        },
        "version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "0.9.1"
        },
        "tsdl_enable": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "enable"
        },
        "PATH": {
            "global": true,
            "resolveAsRelativePath": true,
            "globalCollisionBehavior": "joinPath",
            "val": "./_build/ocamlfind/bin"
        },
        "tsdl_version": {
            "global": true,
            "globalCollisionBehavior": "clobber",
            "val": "0.9.1"
        }
      }
    },
    "devDependencies": {
      "ocaml": "esy-ocaml/ocaml#esy"
    }
}
