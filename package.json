{
    "name": "tsdl-actual", 
    "version": "0.9.1",
    "dependencies": {
        "sdl2": "bsansouci/SDL-mirror.git#esy",
        "@opam/topkg": "*",
        "@opam/ctypes-foreign": "*",
        "@opam/ocamlfind": "*",
        "@opam/ctypes": ">= 0.9.0",
        "substs": "esy-ocaml/substs.git",
        "@opam/ocamlbuild": "*",
        "@opam/result": "*",
        "opam-installer-bin": "esy-ocaml/opam-installer-bin.git"
    },
    "esy": {
      "build": ["ocaml pkg/pkg.ml build --pinned false && (opam-installer --prefix=$opam_prefix || true)"],
      "buildsInSource": true,
      "exportedEnv": {
        "tsdl_actual_version": {
          "val": "0.9.1",
          "scope": "global"
        },
        "tsdl_actual_enable": {
          "val": "enable",
          "scope": "global"
        },
        "tsdl_actual_installed": {
          "val": "true",
          "scope": "global"
        }
      }
    },
    "devDependencies": {
      "ocaml": "esy-ocaml/ocaml#esy"
    }
}
